function disableDropDown(elm){var $select=typeof elm==="object"?elm:$(elm);$select.attr("disabled","disabled");$select.prop("disabled",true);$select.parent().addClass("customSelectDisabled");$select.next().parent().removeClass("option-selected");removeLoader($select);removeErrorPopup($select);resetDropDown($select)}function resetDropDown(elm){var $select=typeof elm==="object"?elm:$(elm);$select.find("option:eq(0)").attr("selected","selected");updateDropDownText($select,$select.find("option:selected").text());$select.next().parent().removeClass("option-selected")}function removeLoader(elm){var $select=typeof elm==="object"?elm:$(elm);$select.closest("ul").removeClass("loader");$select.parent().removeClass("loader");$select.parent().parent().removeClass("loader")}function enableDropDown(elm,data){var $select=typeof elm==="object"?elm:$(elm);if(data){$select.removeAttr("disabled").html(data);$select.find("option:eq(0)").attr("selected","selected");updateDropDownText($select,$select.find("option:selected").text())}else{updateDropDownText($select,$select.find("option:selected").text());$select.removeAttr("disabled")}$select.prop("disabled",false);removeLoader($select);$select.parent().removeClass("customSelectDisabled");$select.next().removeClass("customSelectDisabled")}function enableMultipleDropDown(elms){$(elms).each(function(index,select){enableDropDown(select)})}function disableMultipleDropDown(elms){$(elms).each(function(index,select){disableDropDown(select)})}function updateDropDownText(elm,data){var $elm=typeof elm==="object"?elm:$(elm);$elm.filter(":selected").text(data);$elm.next().find(".customSelectInner").text(data)}function updateCustomDropDownText(skipModel,manufacuturerChanged,manufacturerElm,modelElm,variantElm){updateDropDownChange(manufacuturerChanged,modelElm,variantElm);var variant=$(variantElm).find("option:selected").text();var model=$(modelElm).find("option:selected").val();var manufacturer=$(manufacturerElm).find("option:selected").text();if(vari