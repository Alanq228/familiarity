aEventTracker("Push Discarded",alertString,0)}}}function setJsStorage(value){var current_ttl=$jStorage.getTTL(jsStorageName);ttl=current_ttl?current_ttl:ttl;$jStorage.set(jsStorageName,value);$jStorage.setTTL(jsStorageName,ttl)}function removePopup(){$body.removeClass(bodyClass);$(popupClass).remove()}function getDays(firstDate,secondDate){var oneDay=24*60*60*1e3;var diffDays=Math.round(Math.abs((firstDate-secondDate)/oneDay));return diffDays}function getNotifyToolTip(){if(bInfo.wap){return true}else{var message="Something wrong hapenned, please refresh the page to try again.";if(Notification.permission=="granted"){message=$button.attr("checked")?"Youâve already subscribed.<br> In case you no longer wish to receive notifications,<br> click on the bell icon to unsubscribe.":"Click on the bell icon to subscribe to Auto News."}else{if(!("showNotification"in ServiceWorkerRegistration.prototype)){message="Notifications aren't supported."}if(Notification.permission==="denied"){message='You have blocked notifications for this site.<br/><span><a target="_blank" href="https://support.google.com/chrome/answer/3220216?hl=en&co=GENIE.Platform%3DDesktop&oco=0">Click here for detials.</span>'}if(Notification.permission=="default"){message="You have default browser notifications permission."}}$("#notify-icon-tool-tip").html(message)}}function saveRecordIDB(status){var data={language:bInfo.language,status:status,datetime:(new Date).getTime()};setJsStorage(data);bnIdbQuery.get(bInfo.language).then(function(val){bnIdbQuery.set(bInfo.language,data).then(function(val){if(status==="granted"){showsSuccessAlert()}})}).catch(function(){bnIdbQuery.add(data).then(function(val){if(status==="granted"){showsSuccessAlert()}})})}function saveGAclientIdIDB(clientId){var data={name:"ga-client-id",value:clientId,datetime:(new Date).getTime()};sIdbQuery.get("ga-client-id").then(function(val){if(val.value!==clientId){sIdbQuery.set("ga-client-id",data).catch(function(err){console.error(err)})}}).catch(function(){sIdbQuery.add(data).catch(function(err){console.error(err)})})}function gaEventTracker(eventAction,eventLabel,eventValue){return;eventLabel=eventLabel+" "+(bInfo.wap?"(Mobile)":"(Desktop)");var eventParams={event:"bn_event",eventCategory:"browser-notification",eventAction:eventAction,eventLabel:eventLabel,eventValue:eventValue};if(ga){ga("send","event",eventParams.eventCategory,eventParams.eventAction,eventParams.eventLabel,eventParams.eventValue,{nonInteraction:1})}if(dataLayer){dataLayer.push(eventParams)}}function showsSuccessAlert(){var icon="https://auto.ndtvimg.com/static/media/icons/icon-80.png";var tag=bInfo.language==="hi"?"à¤§à¤¨à¥à¤¯à¤µà¤¾à¤¦":"thankyou";var messgae=bInfo.language==="hi"?"CarandBike à¤à¤²à¤°à¥à¤à¥à¤¸ à¤à¥ à¤¸à¤¦à¤¸à¥à¤¯à¤¤à¤¾ à¤²à¥à¤¨à¥ à¤à¥ à¤²à¤¿à¤ à¤§à¤¨à¥à¤¯à¤µà¤¾à¤¦":"Thank you for subscribing to CarandBike Alerts";var payload={body:messgae,tag:tag,icon:icon};if(bInfo.name==SAFARI){return}else{var notification=new Notification(PRODUCT_NAME,payload)}}function subscribePush(){navigator.serviceWorker.ready.then(function(registration){messaging.requestPermission().then(function(){registerSubscription()}).catch(function(err){saveRecordIDB("denied");$button.attr("checked",false);console.log(err)})})}function saveSubscribedPush(status){var sInfo={};if(bInfo.name==SAFARI){return}else{messaging.getToken().then(function(endPoint){sInfo.deviceToken=endPoint;sInfo.serviceType=serviceTypeFCM;sInfo.status=status;sInfo.topic="all";saveSubscription(sInfo)}).catch(function(err){if(status==="granted"){subscribePush()}else{saveRecordIDB("denied");$button.attr("checked",false)}return})}}function registerSubscription(){messaging.getToken().then(function(endPoint){var data={registration_id:endPoint,service_type:serviceTypeFCM,browser_name:bInfo.name.toLowerCase(),language:bInfo.language,topic:"all"};$.ajax({url:notificationsApiUrl+"/subscribe",type:"POST",data:data,dataType:"json",success:function(data){if(data.msg==="success"){saveRecordIDB("granted")}else{if(data.msg==="error"&&data.info==="show-popup"){showSubscriptionPopup()}}},error:function(data,status,err){console.log(err)}})}).catch(function(err){console.log("Unable to retrieve refreshed token ",err);return})}function saveSubscription(sInfo){var data={registration_id:sInfo.deviceToken,service_type:sInfo.serviceType,browser_name:bInfo.name.toLowerCase(),language:bInfo.language,language_status:sInfo.status,topic:sInfo.topic};$.ajax({url:notificationsApiUrl+"/save-subscription",type:"POST",data:data,dataType:"json",success:function(data){if(data.msg==="success"){saveRecordIDB(sInfo.status);if(bInfo.wap){var tm=new toastMessage({bottomLift:true});var _msg;if(sInfo.status==="granted"){_msg="You're subscribed succesfully.<br> You can click it again to unsubscribe";tm.enqueue({status:"success",text:_msg})}else{_msg="You're unsubscribe succesfully.<br> You can click it again to subscribe";tm.enqueue({status:"warning",text:_msg})}}}else{if(data.msg==="error"&&data.info==="unregister"&&sInfo.status==="granted"){subscribePush()}}},error:function(data,status,err){console.log(err)}})}function removeSubscription(endPoint){if(endPoint){var data={registration_id:endPoint,service_type:serviceTypeFCM,browser_name:bInfo.name.toLowerCase()};$.ajax({url:notificationsApiUrl+"/unsubscribe",type:"POST",data:data,dataType:"json",success:function(data){if(data.msg==="success"){saveRecordIDB("denied")}else{console.log(data.info)}},error:function(data,status,err){console.log(err)}})}else{console.log("Unable to delete token!");return}}function showSubscriptionPopup(){$.ajax({url:notificationsApiUrl+"/index?lang="+bInfo.language,type:"GET",dataType:"html",success:function(data){if(bInfo.wap){$body.append(data);createStyleTag("/static/css/m/component/notification.css?v=1",function(){$(".js-cl-popup-notify").show();bodyClass="h__noscroll";$body.addClass(bodyClass)})}else{$body.append(data);bodyClass="h__noscroll";$body.addClass(bodyClass)}}})}$(document).on("click",subBtn,function(){removePopup();saveSubscribedPush("granted");$button.attr("checked",true)});$(document).on("click",cancelBtn,function(){removePopup();saveSubscribedPush("denied");$button.attr("checked",false)});$(document).on("hover",".js-opts-browser-notify",function(){getNotifyToolTip()});$button.click(function(){if($button.attr("checked")){if(bInfo.name==SAFARI){return}else if(Notification.permission==="granted"){saveSubscribedPush("granted")}else if(Notification.permission==="denied"){console.log("Browser Notifications blocked from browser settings..");$button.attr("checked",false)}else{console.log("Something went wrong with endpoint subscribe..");$button.attr("checked",false)}}else{if(bInfo.name==SAFARI){return}else{bnIdbQuery.vals().then(function(vals){var active_sub=0;for(var i=0;i<vals.length;i++){if(vals[i].status==="granted"){active_sub=active_sub+1}}if(active_sub>1){saveSubscribedPush("denied")}else{messaging.getToken().then(function(currentToken){messaging.deleteToken(currentToken).then(function(){removeSubscription(currentToken)}).catch(function(err){console.log("Unable to delete token. ",err)})}).catch(function(err){console.log("Error retrieving Instance ID token. ",err)})}})}}});this.browserPushNotify=function(options){if(typeof Promise!=="undefined"&&Promise.toString().indexOf("[native code]")!==-1){isValidBrowser().then(function(status){if(status){ga(function(tracker){var clientId=tracker.get("clientId");saveGAclientIdIDB(clientId)});if(bInfo.wap){$button.attr("data-input_status","on")}else{$notifyOpts.removeClass("hidden");$button.attr("data-input_status","on")}navigator.serviceWorker.register(serviceWorkerJsPath).then(function(registration){messaging.useServiceWorker(registration);bnIdbQuery.get(bInfo.language).then(function(val){if(val.status==="granted"&&Notification.permission==="granted"){if(getDays(val.datetime,(new Date).getTime())>30){requestSubscribePermission()}else{$button.attr("checked",true)}}else if(val.status==="default"&&Notification.permission==="default"){$button.attr("checked",false);showSubscriptionPopup()}else if(val.status==="denied"){if(getDays(val.datetime,(new Date).getTime())>7&&Notification.permission==="granted"){showSubscriptionPopup()}else{$button.attr("checked",false)}}else{$button.attr("checked",false)}}).catch(function(){var js_storage_data=$jStorage.get(jsStorageName);if(js_storage_data){return}else{requestSubscribePermission()}})}).catch(function(err){console.log("Service Worker Error",err)});var refreshing;if(navigator.serviceWorker.controller){navigator.serviceWorker.addEventListener("controllerchange",function(){if(refreshing)return;window.location.reload();refreshing=true})}}})}function requestSubscribePermission(){Notification.requestPermission().then(function(permission){if(permission==="granted"){registerSubscription();$button.attr("checked",true)}else if(permission=="default"){saveRecordIDB(permission);$button.attr("checked",false)}else if(permission=="denied"){saveRecordIDB(permission);$button.attr("checked",false)}})}};setTimeout(function(){var typeofModalAd=typeof displayModalAd==="undefined";if(typeofModalAd||!typeofModalAd&&displayModalAd==false){me.browserPushNotify()}},1e3)}function initIndexedDB(_opts){var _default={dbName:"auto_ndtv_bn_db",dbVersion:1,objectStore:"browserPushNotify",indexed:{keyPath:"language"}};var options=$.extend(_default,_opts);var db;function getIDB(){if(!db){db=new Promise(function(resolve,reject){var openreq=indexedDB.open(options.dbName,options.dbVersion);openreq.onerror=function(){reject(openreq.error)};openreq.onupgradeneeded=function(){openreq.result.createObjectStore(options.objectStore,options.indexed)};openreq.onsuccess=function(){resolve(openreq.result)}})}return db}function storage(type,callback){return getIDB().then(function(db){return new Promise(function(resolve,reject){var transaction=db.transaction(options.objectStore,type);transaction.oncomplete=function(){resolve()};transaction.onerror=function(){reject(transaction.error)};callback(transaction.objectStore(options.objectStore))})})}var idbQuery={get:function(key){var req;return storage("readonly",function(store){req=store.get(key)}).then(function(){return req.result})},add:function(value){return storage("readwrite",function(store){store.add(value)})},set:function(key,value){return storage("readwrite",function(store){store.put(value)})},delete:function(key){return storage("readwrite",function(store){store.delete(key)})},clear:function(){return storage("readwrite",function(store){store.clear()})},keys:function(){var keys=[];return storage("readonly",function(store){(store.openKeyCursor||store.openCursor).call(store).onsuccess=function(){if(!this.result)return;keys.push(this.result.key);this.result.continue()}}).then(function(){return keys})},vals:function(){var vals=[];return storage("readonly",function(store){store.openCursor.call(store).onsuccess=function(){if(!this.result)return;vals.push(this.result.value);this.result.continue()}}).then(function(){return vals})}};return idbQuery}initBrowserNotification();